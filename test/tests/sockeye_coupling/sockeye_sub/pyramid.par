[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 2
  dt = 0.2
  polynomialOrder = 2
  writeControl = steps
  writeInterval = 2

[VELOCITY]
  solver = none
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none
  residualTol = 1.0e-5
  boundaryTypeMap = f, f, f, f, f, f, f, f
