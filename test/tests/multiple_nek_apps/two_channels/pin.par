[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 2000
  dt = 0.2
  timeStepper = tombo2
  writeControl = steps
  writeInterval = 5
  polynomialOrder = 1

[VELOCITY]
  solver = none

[PRESSURE]

[TEMPERATURE]
  conductivity = 0.1
  rhoCp = 1.0
  boundaryTypeMap = f, t, I
  residualTol = 1.0e-6
