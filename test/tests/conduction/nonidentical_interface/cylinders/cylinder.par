[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 4
  writeControl = steps
  writeInterval = 50

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  boundaryTypeMap = wall, wall, inlet, outlet

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  conductivity = 2.5
  rhoCp = 1.0
  residualTol = 1.0e-10
  boundaryTypeMap = t, f, I, I
