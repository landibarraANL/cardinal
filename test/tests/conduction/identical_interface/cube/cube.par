[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 10
  dt = 0.1
  polynomialOrder = 5
  writeControl = steps
  writeInterval = 10

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  conductivity = 2.5
  rhoCp = 1.0
  residualTol = 1.0e-10
  boundaryTypeMap = I, I, I, f, I, t
