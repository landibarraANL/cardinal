[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 3
  dt = 1.0
  polynomialOrder = 2
  writeControl = steps
  writeInterval = 1

[MESH]
  solver = user

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  conductivity = 1.0
  rhoCp = 0.0
  residualTol = 1.0e-5
  boundaryTypeMap = t, t, t, t, t, t
