[GENERAL]
  polynomialOrder = 7
  numSteps = 5
  dt = 2e-02
  timeStepper = tombo2
  writeInterval = -1

[PRESSURE]
  residualTol = 1e-06

[VELOCITY]
  boundaryTypeMap = v, O, W
  residualTol = 1e-08
  density = 1.0
  viscosity = 0.001

[TEMPERATURE]
  boundaryTypeMap = t, O, I
  residualTol = 1e-08
  rhoCp = 1.0
  conductivity = 0.001
