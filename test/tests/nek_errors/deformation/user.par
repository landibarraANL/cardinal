[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1
  dt = 1.0
  polynomialOrder = 2
  writeControl = steps
  writeInterval = 1
  oudf = "nekbox.oudf"
  udf = "nekbox.udf"
  verbose = true
[MESH]
  solver = user
  file = "nekbox.re2"

[VELOCITY]
  solver = none
  residualTol = 1.0e-6
  boundaryTypeMap = inlet, outlet, wall, wall, wall, wall

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  solver = none
#  conductivity = 1.0
#  rhoCp = 0.0
#  residualTol = 1.0e-5
#  boundaryTypeMap = I, f, I, I, I, I
