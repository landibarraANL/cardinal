[GENERAL]
polynomialOrder = 2
stopAt = numSteps
numSteps = 1
dt = 1e-3
timeStepper = tombo2
writeInterval = 100
oudf = "nekbox_elast.oudf"
udf = "nekbox.udf"

[MESH]
boundaryTypeMap = zeroValue,zeroValue,zeroValue,zeroValue,zeroValue,zeroValue
solver = pcg+block
file = "nekbox.re2"

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-4

[VELOCITY]
density = 1.0
viscosity = -100.0
boundaryTypeMap = zeroValue,codedFixedValue,zeroValue,zeroValue,zeroValue,zeroGradient
residualTol = 1e-5

[TEMPERATURE]
solver = none
