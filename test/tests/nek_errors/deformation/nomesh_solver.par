[GENERAL]
  polynomialOrder = 2
  stopAt = numSteps
  numSteps = 1
  dt = 1e-3
  timeStepper = tombo2
  writeInterval = 1
  oudf = "nekbox.oudf"
  udf = "nekbox.udf"

[MESH]
  solver = none
  file = "nekbox.re2"

[PROBLEMTYPE]
  stressFormulation = true

[PRESSURE]
  residualTol = 1e-4

[VELOCITY]
  solver = none

[TEMPERATURE]
  solver = none
